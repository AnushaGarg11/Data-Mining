    "def kmeans_clustering(X, n_clusters, init_method, max_iter=300):\n",
    "    mse_values = []\n",
    "    \n",
    "    kmeans = KMeans(n_clusters=n_clusters, init=init_method, max_iter=1, n_init=10, random_state=42)\n",
    "    \n",
    "    for i in range(1, max_iter + 1):\n",
    "        kmeans.max_iter = i\n",
    "        kmeans.fit(X)\n",
    "        mse = kmeans.inertia_ / X.shape[0]\n",
    "        mse_values.append(mse)\n",
    "    \n",
    "    return kmeans.labels_, mse_values\n",
    "\n",
