   "source": [
    "def plot_mse(mse_values, title):\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    plt.plot(range(1, len(mse_values) + 1), mse_values, marker='o')\n",
    "    plt.xlabel('Iteration')\n",
    "    plt.ylabel('Mean Squared Error')\n",
    "    plt.title(title)\n",
    "    plt.grid(True)\n",
    "    plt.show()"
